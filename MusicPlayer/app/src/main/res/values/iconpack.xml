<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <!--
            JMusic contains a theme for Apex and Nova launchers
            This theme is intended to be used to manually select an icon for JMusic that
            matches the chosen theme

            These strings provide information to Apex & Nova
    -->

    <!-- Basic theme information -->
    <string name="theme_title" tools:ignore="UnusedResources">JMusic Alternate Icons</string>
    <string name="developer_name" tools:ignore="UnusedResources">marverenic</string>
    <string name="theme_description" tools:ignore="UnusedResources">
        This theme contains alternate icons for JMusic.\n\nDon\'t set this as your launcher\'s
        theme. You should manually change JMusic\'s icon to your preference if desired.</string>

    <!-- Theme settings -->
    <bool name="config_iconpack" tools:ignore="UnusedResources">true</bool>
    <bool name="config_apexskin" tools:ignore="UnusedResources">false</bool>

    <!-- The feature graphic will appear above the theme description -->
    <string name="theme_feature" tools:ignore="UnusedResources">ic_launcher_black</string>

    <!-- Each theme may have up to 5 preview images -->
    <string name="theme_preview1" tools:ignore="UnusedResources">ic_launcher_red</string>
    <string name="theme_preview2" tools:ignore="UnusedResources">ic_launcher_amber</string>

    <string-array name="icon_pack" translatable="false" tools:ignore="UnusedResources">
        <item>ic_launcher</item>
        <item>ic_launcher_red</item>
        <item>ic_launcher_orange</item>
        <item>ic_launcher_yellow</item>
        <item>ic_launcher_green</item>
        <item>ic_launcher_purple</item>
        <item>ic_launcher_grey</item>
    </string-array>

</resources>